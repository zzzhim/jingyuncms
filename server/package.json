{
  "name": "jingyun-cms-server",
  "version": "1.0.0",
  "description": "jingyun_cms",
  "main": "index.js",
  "scripts": {
    "start": "cross-env APP_NODE_ENV=dev nodemon index.js --exec babel-node",
    "dev": "concurrently npm:nodeDev npm:gulpDev",
    "nodeDev": "cross-env APP_NODE_ENV=dev nodemon index.js --exec babel-node",
    "gulpDev": "cross-env APP_NODE_ENV=dev gulp build",
    "test": "concurrently npm:nodeTest npm:gulpTest",
    "nodeTest": "cross-env APP_NODE_ENV=test nodemon index.js --exec babel-node",
    "gulpTest": "cross-env APP_NODE_ENV=test gulp build",
    "prod": "npm run build && cross-env APP_NODE_ENV=prod node ./dist/index.js",
    "build": "cross-env APP_NODE_ENV=prod webpack --config webpack.config.js",
    "buildStart": "npm run build && cross-env APP_NODE_ENV=prod node ./dist/index.js",
    "exit": "taskkill -PID task -F"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/multer": "^3.0.0",
    "@sentry/node": "^7.1.1",
    "@sentry/tracing": "^7.1.1",
    "axios": "^0.26.0",
    "dayjs": "^1.11.3",
    "form-data": "^4.0.0",
    "hanzi-tools": "^1.2.26",
    "ioredis": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-session": "^6.2.0",
    "koa-static": "^5.0.0",
    "log4js": "^6.4.2",
    "md5": "^2.3.0",
    "mkdirp": "^1.0.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "nodejieba": "^2.6.0",
    "nodemailer": "^6.7.5",
    "puppeteer": "^13.5.0",
    "segment": "^0.1.3",
    "sequelize": "^6.30.0",
    "socket.io": "^4.4.1",
    "svg-captcha": "^1.4.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
